{
  "env": {
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base", "plugin:jest/recommended", "plugin:prettier/recommended"],
  "plugins": ["jest", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": ["warn", "double"],
    "no-unused-vars": "warn"
  }
}
